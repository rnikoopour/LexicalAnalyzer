class DFSM:
    def __init__(states, alphabet, trans_funcs, start_state, accept_states):
        self.states = states
        self.alphabet = alphabet
        self.trans_func = trans_function
        self.start_state = start_state
        self.current_state = start_state
        self.accept_states = accept_states

    def Transition(input):
        new_state = None
        for function in self.trans_func:
            if function.start_state is self.current_state \
               and function.input is input:
                new_state = function.next_state
                break
        return new_state
